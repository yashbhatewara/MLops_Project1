{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "TempAllowFirstProj",
      "Effect": "Allow",
      "Principal": {"AWS": "arn:aws:iam::040865382580:user/firstproj"},
      "Action": ["s3:ListBucket","s3:GetBucketLocation","s3:GetBucketAcl","s3:GetBucketPolicy"],
      "Resource": ["arn:aws:s3:::abc"]
    },
    {
      "Sid": "TempAllowObjects",
      "Effect": "Allow",
      "Principal": {"AWS": "arn:aws:iam::040865382580:user/firstproj"},
      "Action": ["s3:GetObject","s3:PutObject","s3:DeleteObject","s3:GetObjectAcl"],
      "Resource": ["arn:aws:s3:::abc/*"]
    }
  ]
}
